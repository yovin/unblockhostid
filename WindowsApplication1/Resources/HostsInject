@echo off

SET NEWLINE=^& echo.

ECHO %NEWLINE%^ >>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "cfs.uzone.id" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 cfs.uzone.id>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "internetpositif.uzone.id" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 internetpositif.uzone.id>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "uzone.id" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 uzone.id>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "cfs2.uzone.id" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 cfs2.uzone.id>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "u-ad.info" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 u-ad.info>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "cfs.u-ad.info" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 cfs.u-ad.info>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "cfs2.uadexchange.com" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 cfs2.uadexchange.com>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "www.internetpositif.uzone.id" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 www.internetpositif.uzone.id>>%WINDIR%\System32\drivers\etc\hosts

FIND /C /I "www.uzone.id" %WINDIR%\system32\drivers\etc\hosts
IF %ERRORLEVEL% NEQ 0 ECHO %NEWLINE%^127.0.0.1 www.uzone.id>>%WINDIR%\System32\drivers\etc\hosts

pause